.create
  %header.purchaseHead
    .purchaseHead__logo
      = link_to image_tag("logo.png", alt: "FURIMA"), root_path
  .create__frame
    .create__frame__message
      登録が完了しました。
    %ul.create__frame__btns
      %li.my_page_link
        = link_to "マイページに戻る", user_path(current_user.id), class: "my_page_link__btn"
      %li.credit_cards_index_link
        = link_to "登録カード一覧", credit_card_path(current_user.id), class: "credit_cards_index_link__btn"